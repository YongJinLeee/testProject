# Uncomment the next line to define a global platform for your project
# platform :ios, '9.0'

def share_pods

  # UI
  pod 'SnapKit'

  # RxSwift, Rxcocoa
  pod 'RxSwift', '6.2.0'
  pod 'RxCocoa', '6.2.0'
  
  # Image
  pod 'SDWebImage'
  pod 'Kingfisher'

  # Network
  pod 'Alamofire', '5.5.0'
  pod 'Moya', '15.0.0'
  
end

target 'testProject' do

  use_frameworks!
  inhibit_all_warnings!

  share_pods
end
